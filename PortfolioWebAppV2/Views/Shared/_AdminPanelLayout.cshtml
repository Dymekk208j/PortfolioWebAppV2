    <!-- all line chart activation -->
    <script src="~/Lib/assets/js/line-chart.js"></script>

    <!-- all pie chart -->
    <script src="~/Lib/assets/js/pie-chart.js"></script>

    <!-- others plugins -->
    <script src="~/Lib/assets/js/plugins.js"></script>
